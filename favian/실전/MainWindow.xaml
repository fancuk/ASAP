<Window x:Class="실전.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:실전"
        mc:Ignorable="d"
        Title="다민 Messenger" Height="138" Width="283.467">
    <Grid>
        <Button x:Name="sign_Up" Content="Sign Up" Command="{Binding _sign_Up_Page}" HorizontalAlignment="Left" Margin="10,68,0,0" VerticalAlignment="Top" Width="108"/>
        <Button x:Name="log_In" Content="Log in" Command="{Binding _log_In_Page}" HorizontalAlignment="Left" Margin="138,67,0,0" VerticalAlignment="Top" Width="114"/>
        <TextBox x:Name="id" HorizontalAlignment="Left" Height="23" Margin="67,10,0,0" Text="{Binding log_In_ID}" TextWrapping="Wrap"  VerticalAlignment="Top" Width="185"/>
        <TextBlock x:Name="idBlock" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="ID" VerticalAlignment="Top"/>
        <TextBlock x:Name="passwordBlock" HorizontalAlignment="Left" Margin="10,41,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top"/>
        <TextBox x:Name="password" HorizontalAlignment="Left" Height="23" Margin="67,40,0,0" TextWrapping="Wrap" Text="{Binding log_In_Password}" VerticalAlignment="Top" Width="185"/>

    </Grid>
</Window>
